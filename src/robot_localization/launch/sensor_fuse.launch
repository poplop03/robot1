<launch>
  <!-- Use tf2_ros instead of tf -->
  <node pkg="tf2_ros" type="static_transform_publisher" name="base_footprint_to_base_link"
        args="0 0 0.062 0 0 0 base_footprint base_link" />

  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true" output="screen">
    <rosparam command="load" file="$(find robot_localization)/params/sensor_fuse_config.yaml" />
  </node>
</launch>

